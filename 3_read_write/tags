buffer_tail	read_write.c	/^static int buffer_tail;$/;"	v	file:
driver_close	read_write.c	/^static int driver_close(struct inode *device_file, struct file *instance) {$/;"	f	file:
driver_open	read_write.c	/^static int driver_open(struct inode *device_file, struct file *instance) {$/;"	f	file:
driver_read	read_write.c	/^static ssize_t driver_read(struct file* fp, char* user_buf, size_t count, loff_t *offset) {$/;"	f	file:
driver_write	read_write.c	/^static ssize_t driver_write(struct file* fp, const char* user_buf, size_t count, loff_t *offset) {$/;"	f	file:
fops	read_write.c	/^static struct file_operations fops = {$/;"	v	typeref:struct:file_operations	file:
